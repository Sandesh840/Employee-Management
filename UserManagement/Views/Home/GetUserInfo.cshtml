<h1>INNER JOIN</h1>
<table class="table">
    <thead>
        <tr>
            <th>User name</th>
            
            <th>Department Name</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.UserInfo)
        {
            <tr>
                <td>@item.User_Name</td>               
                <td>@item.Department_Name</td>
                <td>@item.Salary</td>
            </tr>            
        }
    </tbody>
</table>

